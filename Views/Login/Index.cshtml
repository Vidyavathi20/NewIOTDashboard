<script>
     $(function () {
       var url2 = '@Url.Action("Getrole","Roles")';
        $.ajax({
            type: "POST",
            url: url2,
            data: '{}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                var s = '<option value="">Please Select</option>';
