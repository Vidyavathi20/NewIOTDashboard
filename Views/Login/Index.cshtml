                    s += '<option value="' + data[i].id + '">' + data[i].Userrole + ' </option>';
                }
                $("#eUserrole").html(s);

                            }
        });
    });

    $(function () {
       var url2 = '@Url.Action("Getemp", "Login")';
        $.ajax({
            type: "POST",
            url: url2,
            data: '{}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                var s = '<option value="">Please Select</option>';
                for (var i = 0; i < data.length; i++) {
                    s += '<option value="' + data[i].id + '">' + data[i].Employeename + ' </option>';
                }
                $("#Empname").html(s);

                            }
        });
    });

</script>
<script>

    $("#add").click(function () {
        $("#divnew").show();

    });

    $(".cancel1").click(function () {
        $("#divnew").hide();
    });

    $(".cancel").click(function () {
        $("#divnew1").hide();

    });

    function Getemp() {
        var id = $("#Empname").val();
        var url = '@Url.Action("Get_Email","Login")';
        $.ajax({
            type: "POST",
            url: url,
            contentType: "application/json; charset=utf-8",
            dataType: 'json',
            data: JSON.stringify({ id: id }),
            success: function (response) {
               

            },
            error: function (xhr, exception) {
                
            }

        });
    }

    function Checkrole()
    {
        var id = $("#Empname").val();
        var url = '@Url.Action("Check_role", "Login")';
        $.ajax({
            type: "POST",
            url: url,
            contentType: "application/json; charset=utf-8",
            dataType: 'json',
            data: JSON.stringify({ id: id }),
            success: function (response) {

                var res = JSON.parse(response);
                var a = $.trim(res[0]["Userrole"]);

                $("#role_name").val(a);
               
                if (a == "Admin")
                {
                    $("#pwd").show();
                    $("#div_forgot").show();
                }
                else {
                    $("#pwd").hide();
                    $("#div_forgot").hide();
                }
            },
            error: function (xhr, exception) {
               
            }
        });
    }

    $("#signin").click(function ()
    {   

        var id = $("#Empname").val();

        if (id == "") {
            document.getElementById("err_name").innerHTML = "Required";
            return false;
        }
        else {
            document.getElementById("err_name").innerHTML = "";
        }

        var role_name = $("#role_name").val();

        if (role_name == "Admin")
        {

            var Password = $("#Password").val();

            if (Password == "") {
                document.getElementById("errMsg").innerHTML = "Required";
                return false;
            }
            else {
                document.getElementById("errMsg").innerHTML = "";
            }
        }
    });

    

</script>

